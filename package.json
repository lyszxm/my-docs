{
  "name": "my-press-demo",
  "version": "0.0.2",
  "description": "用来面试以及其它项目使用的",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "release": "release-it",
    "build": "vuepress build src",
    "docs:clean-dev": "vuepress dev src --clean-cache",
    "dev": "vuepress dev src",
    "docs:update-package": "npx vp-update",
    "prepare": "husky install",
    "deploy-gh": "yarn docs:build && bash scripts/deploy-gh.sh",
    "tsc": "tsc -p ./tsconfig.json",
    "ts": "nodemon --watch src/ -e ts --exec ts-node ./src/zh/typescript/cw_ts数据结构/AllCode_ns/index.ts",
    "clean_node_modules": "rimraf node_modules"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@vuepress/bundler-vite": "^2.0.0-rc.14",
    "@vuepress/client": "^2.0.0-rc.14",
    "@vuepress/plugin-docsearch": "^2.0.0-rc.37",
    "@vuepress/plugin-photo-swipe": "^2.0.0-rc.37",
    "@vuepress/plugin-search": "^2.0.0-rc.37",
    "cz-customizable": "6.3.0",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.1",
    "lint-staged": "^9.5.0",
    "pathe": "^1.0.0",
    "prettier": "^3.3.2",
    "vue": "^3.4.29",
    "vuepress": "^2.0.0-rc.14",
    "vuepress-plugin-search-pro": "^2.0.0-rc.37",
    "vuepress-theme-hope": "^2.0.0-rc.37"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "bundler-vite": "link:@vuepress\\bundler-vite",
    "cosmiconfig": "^7.1.0",
    "path": "^0.12.7",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.2",
    "utils": "link:@vuepress\\utils"
  }
}