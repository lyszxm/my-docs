{
  "name": "my-press-demo",
  "version": "0.0.2",
  "description": "用来面试以及其它项目使用的",
  "license": "MIT",
  "scripts": {
    "build": "vuepress build src",
    "docs:clean-dev": "vuepress dev src --clean-cache",
    "dev": "vuepress dev src",
    "docs:update-package": "npx vp-update",
    "prepare": "husky install",
    "deploy-gh": "yarn docs:build && bash scripts/deploy-gh.sh"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@vuepress/client": "2.0.0-beta.61",
    "@vuepress/plugin-docsearch": "^2.0.0-beta.61",
    "@vuepress/plugin-search": "^2.0.0-beta.61",
    "cz-customizable": "6.3.0",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.1",
    "lint-staged": "^9.5.0",
    "pathe": "^1.0.0",
    "prettier": "^2.8.6",
    "vue": "^3.2.47",
    "vuepress": "2.0.0-beta.61",
    "vuepress-plugin-components": "^2.0.0-beta.61",
    "vuepress-plugin-photo-swipe": "^2.0.0-beta.197",
    "vuepress-plugin-search-pro": "^2.0.0-beta.197",
    "vuepress-theme-hope": "2.0.0-beta.197"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "cosmiconfig": "^7.1.0",
    "typescript": "^5.0.2"
  }
}
